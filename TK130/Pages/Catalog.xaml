                        <TreeViewItem></TreeViewItem>
                        <TreeViewItem></TreeViewItem>
                        <TreeViewItem></TreeViewItem>
                        <TreeViewItem></TreeViewItem>
                        <TreeViewItem></TreeViewItem>
                    </TreeViewItem><TreeViewItem></TreeViewItem>
                    <TreeViewItem Header="Установка дизеля Д120 на тракторе"></TreeViewItem>
                    <TreeViewItem Header="Установка воздухоочистителя"></TreeViewItem>
                    <TreeViewItem Header="Управление двигателем"></TreeViewItem>
                    <TreeViewItem Header="Муфта сцепления"></TreeViewItem>
                    <TreeViewItem Header="Управление муфтой сцепления"></TreeViewItem>
                    <TreeViewItem Header="Привод насосов"></TreeViewItem>
                    <TreeViewItem Header="Полурама"></TreeViewItem>
